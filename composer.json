    "license": "MIT",
    "authors": [
        {
            "name": "Mayowa Tudonu",
            "email": "mtudonu85@gmail.com"
        }
    ],
    "type": "library",
    "require": {
		"php": ">=7.0"
    },
	"require-dev": {
        "phpunit/phpunit": "^6"
    },
	"autoload": {
        "psr-4": {
            "mayorcoded\\parallec\\": "src/",
            "mayorcoded\\parallec\\Exceptions\\": "src/Exceptions/",
            "mayorcoded\\parallec\\Utilities\\": "src/Utilities/"
        }
    }
}
